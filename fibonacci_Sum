num = int(input("Enter the number of Fibonacci numbers you want: "))

if num <= 0:
    print("Please enter a positive integer.")
elif num == 1:
    print("Sum of first 1 Fibonacci number is: 0")
else:
    a = 0
    b = 1
    sum = a + b
    for i in range(2, num):
        s = a + b
        sum += s
        a = b
        b = s
    print("Sum of all numbers in Fibonacci series is:", sum)
